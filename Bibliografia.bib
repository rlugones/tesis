	journal = {J. Geophys. Res.},
	author = {Miller, J. A. and Cargill, P. J. and Emslie, A. G. and Holman, G. D. and Dennis, B. R. and LaRosa, T. N. and Winglee, R. M. and Benka, S. G. and Tsuneta, S.},
