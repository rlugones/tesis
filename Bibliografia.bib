@article{mininni_isotropization_2012,
	title = {Isotropization at small scales of rotating helically driven turbulence},
	volume = {699},
	issn = {1469-7645},
	url = {http://journals.cambridge.org/article_S0022112012000997},
	doi = {10.1017/jfm.2012.99},
	abstract = {We present numerical evidence of how three-dimensionalization occurs at small scale in rotating turbulence with Beltrami (\${\textbackslash}mathitABC\$) forcing, creating helical flow. The Zeman scale \${\textbackslash}ell \_{\O}mega \$ at which the inertial and eddy turn-over times are equal is more than one order of magnitude larger than the dissipation scale, with the relevant domains (large-scale inverse cascade of energy, dual regime in the direct cascade of energy \$E\$ and helicity \$H\$, and dissipation) each moderately resolved. These results stem from the analysis of a large direct numerical simulation on a grid of \$3072{\textasciicircum}3 \$ points, with Rossby and Reynolds numbers, respectively, equal to \$0. 07\$ and \$2. 7{\textbackslash}ensuremath{\textbackslash}times 10{\textasciicircum}4 \$. At scales smaller than the forcing, a helical wave-modulated inertial law for the energy and helicity spectra is followed beyond \${\textbackslash}ell \_{\O}mega \$ by Kolmogorov spectra for \$E\$ and \$H\$. Looking at the two-dimensional slow manifold, we also show that the helicity spectrum breaks down at \${\textbackslash}ell \_{\O}mega \$, a clear sign of recovery of three-dimensionality in the small scales.},
	urldate = {2016-08-02},
	journal = {Journal of Fluid Mechanics},
	author = {Mininni, P. D. and Rosenberg, D. and Pouquet, A.},
	month = may,
	year = {2012},
	pages = {263--279},
	file = {Cambridge Journals Snapshot:/home/rodrigo/.mozilla/firefox/d3yzv2sc.default-1445011580632/zotero/storage/RUICKS8J/displayAbstract.html:text/html;mininni2012.pdf:/home/rodrigo/.mozilla/firefox/d3yzv2sc.default-1445011580632/zotero/storage/3NC3PGRW/mininni2012.pdf:application/pdf}
}

