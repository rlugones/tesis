	pages = {294--306},
	file = {NASA/ADS Full Text PDF:/home/rodrigo/.mozilla/firefox/d3yzv2sc.default-1445011580632/zotero/storage/479ZBH2M/Bieber et al. - 1994 - Proton and electron mean free paths The Palmer co.pdf:application/pdf}
}

@article{clark_di_leoni_quantification_2014,
