	keywords = {2104 Cosmic rays, 2134 Interplanetary magnetic fields, 7839 Nonlinear phenomena, 7863 Turbulence, 7924 Forecasting, Decorrelation, Eulerian frame, magnetic field, Solar Wind, turbulence},
	pages = {3995--4004},
	file = {Snapshot:/home/rodrigo/.mozilla/firefox/d3yzv2sc.default-1445011580632/zotero/storage/WDR3IN3D/abstract.html:text/html}
